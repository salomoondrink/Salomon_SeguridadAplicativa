a=[8,9,10,6,6,7,8,
   9,5,6,7,6,8,8,9,
   9,5,6,7,8,10,7,8,
   9,9]



def promedio(a):
    suma=0
    for num in a:
        suma=suma+num
    prom=suma/len(a)
    print("EL promedio es",prom)


def reprobaron(a):
    x=0
    for rep in a:
        if(rep<=5):
           x=x+1
    print("Los alumnos reprobados son",x)
            


promedio(a)
reprobaron(a) 
    

